generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Listing {
  id           Int      @id @default(autoincrement())
  title        String
  description  String
  city         String
  price        Int
  totalSpots   Int
  filledSpots  Int      @default(0)
  createdAt    DateTime @default(now())
}
